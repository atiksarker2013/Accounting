<Window x:Class="CHART_ACC_UI.UI.CustomerEmployeeModify"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Customer/Employee Modify" Height="323" Width="478" WindowStartupLocation="CenterScreen" KeyUp="Window_KeyUp" Background="{StaticResource GrayWhite}" ResizeMode="CanMinimize" ShowInTaskbar="False" Closing="Window_Closing">
    <Grid>
        <Button Content=".." Height="23" HorizontalAlignment="Left" Margin="326,17,0,0" Name="btnSearch" VerticalAlignment="Top" Width="28" Click="btnSearch_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="133,50,0,0" Name="txtName" VerticalAlignment="Top" Width="221" TabIndex="0" Style="{StaticResource GraywithBlueBorderTextBox}" />
        <Label Content="Cust/Emp. Name" Height="28" HorizontalAlignment="Left" Margin="25,46,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="133,85,0,0" Name="txtContactNo" VerticalAlignment="Top" Width="220" TabIndex="1" Style="{StaticResource GraywithBlueBorderTextBox}" />
        <Label Content="Contact No." Height="28" HorizontalAlignment="Left" Margin="25,80,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="133,155,0,0" Name="txtNationalId" VerticalAlignment="Top" Width="220" TabIndex="2" Style="{StaticResource GraywithBlueBorderTextBox}" />
        <Label Content="National Id" Height="28" HorizontalAlignment="Left" Margin="25,155,0,0" Name="label4" VerticalAlignment="Top" />
        <Label Content="Account No" Height="28" HorizontalAlignment="Left" Margin="25,12,0,0" Name="label8" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="133,17,0,0" Name="txtAccountCode" VerticalAlignment="Top" Width="189" TabIndex="6" Style="{StaticResource GraywithBlueBorderTextBox}" IsEnabled="False" />
        <Button Content="Update" Height="23" HorizontalAlignment="Left" Margin="133,246,0,0" Name="btnUpdate" VerticalAlignment="Top" Width="68" Click="btnUpdate_Click" TabIndex="10" />
        <Button Content="Reset" Height="23" HorizontalAlignment="Left" Margin="208,247,0,0" Name="btnReset" VerticalAlignment="Top" Width="68" Click="btnReset_Click" TabIndex="11" />
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="286,247,0,0" Name="btnCancel" VerticalAlignment="Top" Width="68" Click="btnCancel_Click" TabIndex="12" />
        <Label Content="Address" Height="28" HorizontalAlignment="Left" Margin="27,193,0,0" Name="label9" VerticalAlignment="Top" />
        <TextBox Height="54" HorizontalAlignment="Left" Margin="133,184,0,0" Name="txtAddress" VerticalAlignment="Top" Width="220" TabIndex="3" Style="{StaticResource GraywithBlueBorderTextBox}" AcceptsReturn="True" TextWrapping="Wrap"/>
        <Label Content="Contact Person" Height="28" HorizontalAlignment="Left" Margin="27,118,0,0" Name="label11" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="133,119,0,0" Name="txtContactPerson" VerticalAlignment="Top" Width="220" Style="{StaticResource GraywithBlueBorderTextBox}" />
    </Grid>
</Window>
