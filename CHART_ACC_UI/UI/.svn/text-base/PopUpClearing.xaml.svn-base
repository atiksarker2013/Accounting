<Window x:Class="CHART_ACC_UI.UI.PopUpClearing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Inward/Outward Clearing" Height="290" Width="369" Background="{StaticResource GrayWhite}" Closing="Window_Closing" KeyUp="Window_KeyUp" ShowInTaskbar="False" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Clearing Method" Height="28" HorizontalAlignment="Left" Margin="10,71,0,0" Name="label1" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="115,71,0,0" Name="cmbClearingMethod" VerticalAlignment="Top" Width="210" />
        <Button Content="Next" Height="23" HorizontalAlignment="Left" Margin="115,125,0,0" Name="btnNext" VerticalAlignment="Top" Width="75" Click="btnNext_Click" />
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="208,125,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
