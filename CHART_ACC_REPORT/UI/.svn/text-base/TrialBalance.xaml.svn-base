<Window x:Class="CHART_ACC_REPORT.UI.TrialBalance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Trial Balance" Height="256" Width="412" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen" KeyUp="Window_KeyUp" Background="{StaticResource GrayWhite}" ShowInTaskbar="False" Closing="Window_Closing">
    <Grid>
        <Label Content="Fiscal Period" Height="23" HorizontalAlignment="Left" Margin="28,13,0,0" Name="label1" VerticalAlignment="Top" Width="75" />
        <TextBox Height="25" HorizontalAlignment="Left" Margin="108,14,0,0" Name="txtFiscalStartDate" VerticalAlignment="Top" Width="110" IsReadOnly="True" Foreground="Green" IsEnabled="False" />
        <Label Content="to" Height="27" HorizontalAlignment="Left" Margin="216,13,0,0" Name="label2" VerticalAlignment="Top" Width="25" />
        <TextBox Height="25" HorizontalAlignment="Left" Margin="242,13,0,0" Name="txtFiscalEndDate" VerticalAlignment="Top" Width="110" IsReadOnly="True" Foreground="Green" IsEnabled="False" />
        <Label Content="End Date" Height="24" HorizontalAlignment="Left" Margin="80,102,0,0" Name="label3" VerticalAlignment="Top" Width="58" />
        <DatePicker Height="23" HorizontalAlignment="Left" Margin="149,103,0,0" Name="dtpPeriod" VerticalAlignment="Top" Width="173" />
        <Button Content="Preview" Height="22" HorizontalAlignment="Left" Margin="150,174,0,0" Name="btnPreview" VerticalAlignment="Top" Width="80" Click="btnPreview_Click" />
        <Label Content="Start Date" Height="24" HorizontalAlignment="Left" Margin="74,69,0,0" Name="label4" VerticalAlignment="Top" Width="70" />
        <DatePicker Height="23" HorizontalAlignment="Left" Margin="149,69,0,0" Name="dtpFiscalStartDate" VerticalAlignment="Top" Width="173" />
        <Button Content="Close" Height="22" HorizontalAlignment="Left" Margin="241,174,0,0" Name="btnClose" VerticalAlignment="Top" Width="80" Click="btnClose_Click" />
        <CheckBox Content="Transaction Only" Height="16" HorizontalAlignment="Left" Margin="149,138,0,0" Name="chkTransaction" VerticalAlignment="Top" Width="173" />
    </Grid>
</Window>
