<Window x:Class="CHART_ACC_REPORT.UI.Depriciation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Depreciation [Report]" Height="253" Width="420" ShowInTaskbar="False" ResizeMode="CanMinimize" Background="{StaticResource GrayWhite}" WindowStartupLocation="CenterScreen" KeyUp="Window_KeyUp" Closing="Window_Closing">
    <Grid>
        <Label Content="Select Fiscal Year" Height="27" HorizontalAlignment="Left" Margin="36,47,0,0" Name="label1" VerticalAlignment="Top" Width="101" />
        <ComboBox Height="25" HorizontalAlignment="Left" Margin="143,48,0,0" ToolTip="Select fiscal year..." SelectionChanged="cmbFiscalYear_SelectionChanged" DisplayMemberPath="F_Year_Name" Name="cmbFiscalYear" VerticalAlignment="Top" Width="195" />
        <Button Content="Preview" Height="24" HorizontalAlignment="Left" Margin="150,124,0,0" Name="btnPreview" VerticalAlignment="Top" Width="90" Click="btnPreview_Click" />
        <Button Content="Close" Height="24" HorizontalAlignment="Left" Margin="252,124,0,0" Name="btnClose" VerticalAlignment="Top" Width="90" Click="btnClose_Click" />
        <TextBlock Height="22" Margin="143,82,60,0" Name="textBlock1" Text="" VerticalAlignment="Top" />
    </Grid>
</Window>
